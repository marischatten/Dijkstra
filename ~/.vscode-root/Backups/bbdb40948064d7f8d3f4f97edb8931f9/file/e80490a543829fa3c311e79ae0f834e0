#include <iostream>

int main()
{

    int x = 2;
    int y = 3;
    int soma = x + y;

    for (int i = 0; i < 10; i++)
    {
        std::cout << "esse eh o conteudo da soma: " << soma << "\n";
    }

    return 0;
}